pragma solidity ^0.8.0;

contract Utils {

    using Address for address;

    modifier onlyUser(){
        require(!address(msg.sender).isContract(), 'must be user');
        require(msg.sender == tx.origin, "proxy contract not allowed");
        _;
    }

    function calcStartSize(uint256 num, uint256 pageSize, uint256 currentPage) pure public returns (uint256 start, uint256 size) {
        if (num > 0) {
            start = (currentPage - 1) * pageSize;
            require(start < num, 'start error!');
            uint256 end = start + pageSize;
            size = pageSize;
            if (end > num) {
                end = num;
                size = end - start;
            }
        }
    }
}

pragma solidity ^0.8.0;

contract StakingPool is Ownable, ERC721Holder, Utils {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    ISettings public settings;
    IERC721Burnable public nft;
    IERC20 public amt;

    // pool
    struct StakeTokenInfo {
        uint256 amount;
        uint256 depositRewarded;
        uint256 rewardDebt;
        uint256 pending;
    }

    // pid=>tokenId=>--stakeToken
    mapping(uint256 => StakeTokenInfo) public stakeTokens;
    mapping(address => uint256[]) public userTokenIds;

    uint256 private _totalSupply;
    uint256 public totalStake;


    uint256 public starttime;
    uint256 public daySeconds = 86400;
    uint256 public rewardRate = 14058 * 1e18 / daySeconds;  // â‰ˆ 81018518518518512
    uint256 public lastUpdateTime;
    uint256 public rewardPerTokenStored;
    mapping(uint256 => uint256) private _balances;
    mapping(uint256 => uint256) public userRewardPerTokenPaid;
    mapping(uint256 => uint256) public rewards;
    mapping(uint256 => uint256) public deposits;

    event Deposit(address indexed user, uint256 amount);
    event Withdraw(address indexed user, uint256 tokenId);
    event ReclaimStakingReward(address user, uint256 tokenId, uint256 amount);
    event EmergencyToken(address token, address to, uint256 amount);
    event EmergencyTokenIds(address to, uint256[] tokenIds);
    event RewardPaid(address indexed user, uint256 reward);

    constructor(address _settings, address _factory, address _amt) public {
        settings = ISettings(_settings);
        nft = IERC721Burnable(_factory);
        amt = IERC20(_amt);
    }

    function setSettings(address _settings) public onlyOwner {
        require(_settings != address(0), "!Zero");
        settings = ISettings(_settings);
    }

    function setAmt(address amt_) public onlyOwner {
        require(amt_ != address(0), "!Zero");
        amt = IERC20(amt_);
    }

    function setFactory(address _nft) public onlyOwner {
        require(_nft != address(0), "!Zero");
        nft = IERC721Burnable(_nft);
    }

    function _getTokensPower(uint256 tokenId) private returns (uint256 power){
        uint8 quality = uint8(tokenId >> 8);
        power = settings.stakePower(quality);
    }

    function totalSupply() public view returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(uint256 tokenId) public view returns (uint256) {
        return _balances[tokenId];
    }

    modifier checkStart() {
        require(block.timestamp >= starttime, 'AMT: not start');
        _;
    }

    modifier updateReward(uint256 tokenId) {
        rewardPerTokenStored = rewardPerToken();
        lastUpdateTime = block.timestamp;
        rewards[tokenId] = earned(tokenId);
        userRewardPerTokenPaid[tokenId] = rewardPerTokenStored;
        _;
    }

    function rewardPerToken() public view returns (uint256) {
        if (totalSupply() == 0) {
            return rewardPerTokenStored;
        }
        return
        rewardPerTokenStored.add(
            block.timestamp
            .sub(lastUpdateTime)
            .mul(rewardRate)
            .mul(1e18)
            .div(totalSupply())
        );
    }

    function earned(uint256 tokenId) public view returns (uint256) {
        return
        balanceOf(tokenId)
        .mul(rewardPerToken().sub(userRewardPerTokenPaid[tokenId]))
        .div(1e18)
        .add(rewards[tokenId]);
    }


    function deposit(uint256[] memory ids) onlyUser public {
        for (uint i = 0; i < ids.length; i++) {
            _deposit(ids[i]);
            totalStake = totalStake + 1;
        }
    }

    // stake visibility is public as overriding LPTokenWrapper's stake() function
    function _deposit(uint256 tokenId)
    private
    updateReward(tokenId)
    checkStart
    {
        userTokenIds[msg.sender].push(tokenId);
        uint256 _power = _getTokensPower(tokenId);
        uint256 newDeposit = deposits[tokenId].add(_power);
        deposits[tokenId] = newDeposit;
        _totalSupply = _totalSupply.add(_power);
        _balances[tokenId] = _balances[tokenId].add(_power);
        nft.safeTransferFrom(msg.sender, address(this), tokenId);
        StakeTokenInfo storage stakeToken = stakeTokens[tokenId];
        stakeToken.amount = _power;
        emit Deposit(msg.sender, tokenId);
    }

    function withdraw(uint256[] memory ids) onlyUser public {
       for (uint i = 0; i < ids.length; i++) {
            _withdraw(ids[i]);
            totalStake = totalStake - 1;
        }
    }

    function _withdraw(uint256 tokenId)
    private
    updateReward(tokenId)
    checkStart
    {
        require(removeUserId(tokenId), '_sBToken:tokenId error!');
        uint256 _power = _getTokensPower(tokenId);
        deposits[tokenId] = deposits[tokenId].sub(_power);
        _totalSupply = _totalSupply.sub(_power);
        _balances[tokenId] = _balances[tokenId].sub(_power);
        nft.safeTransferFrom(address(this), msg.sender, tokenId);
        userRewardPerTokenPaid[tokenId] = 0;
        rewards[tokenId] = 0;
        emit Withdraw(msg.sender, tokenId);
    }

    function stakeBurnTokens(uint256[] memory ids) onlyUser public {
        for (uint i = 0; i < ids.length; i++) {
            _stakeBurnToken(ids[i]);
            totalStake = totalStake - 1;
        }
    }

    function _stakeBurnToken(uint256 tokenId) private updateReward(tokenId) {
        require(removeUserId(tokenId), '_sBToken:tokenId error!');

        uint256 _power = _getTokensPower(tokenId);
        deposits[tokenId] = deposits[tokenId].sub(_power);
        _totalSupply = _totalSupply.sub(_power);
        _balances[tokenId] = _balances[tokenId].sub(_power);

        uint256 pendingAmt = earned(tokenId);
        getReward(tokenId);

        nft.burn(tokenId);
        delete stakeTokens[tokenId];

        emit ReclaimStakingReward(msg.sender, tokenId, pendingAmt);
    }

    function getReward(uint256 tokenId) private updateReward(tokenId) checkStart {
        uint256 reward = earned(tokenId);
        if (reward > 0) {
            rewards[tokenId] = 0;
            amt.safeTransfer(msg.sender, reward);
            emit RewardPaid(msg.sender, reward);
        }
    }

    //get pending info
    function pending(address _user) public view returns (uint256[] memory details, uint256 total)  {
        details = new uint256[](userTokenIds[_user].length);
        for (uint i = 0; i < userTokenIds[_user].length; i++) {
            uint256 _pendingAmt = _pending(userTokenIds[_user][i]);
            details[i] = _pendingAmt;
            total = total + _pendingAmt;
        }
    }

    function pendingOne(uint256 tokenId) public view returns (uint256){
        return _pending(tokenId);
    }

    function pendingBatch(uint256[] memory ids) public view returns (uint256){
        uint256 pendingBatch_ = 0;
        for (uint i = 0; i < ids.length; i++){
            pendingBatch_ = pendingBatch_ + _pending(ids[i]);
        }
        return pendingBatch_;
    }

    function _pending(uint256 tokenId) private view returns (uint256){
        return earned(tokenId);
    }

    function getUserStakingInfo(address user) public view returns (uint[] memory){
        return userTokenIds[user];
    }

    function getUserStakingTokenIndex(address user, uint256 id) public view returns (uint256 idx) {
        uint256 idx = 0;
        for (uint256 i = 0; i <= userTokenIds[user].length; i++) {
            if (userTokenIds[user][i] == id){
                return idx;
            }
            idx += 1;
        }
        return idx;
    }

    function getUserStakingTokens(address user, uint256 pageSize, uint256 currentPage) public view returns (uint[] memory ids, uint8[] memory qualities, uint8[] memory indexs){
        uint256 start;
        uint256 size;
        (start, size) = calcStartSize(userTokenIds[user].length, pageSize, currentPage);
        ids = new uint[](size);
        qualities = new uint8[](size);
        indexs = new uint8[](size);
        for (uint i = 0; i < size; i++) {
            uint256 tokenId = userTokenIds[user][start + i];
            ids[i] = tokenId;
            qualities[i] = uint8(tokenId >> 8);
            indexs[i] = uint8(tokenId);
        }
    }

    function removeUserId(uint256 tokenId) private returns (bool)  {
        uint256 len = userTokenIds[msg.sender].length;
        for (uint i = 0; i < len; i++) {
            if (tokenId == userTokenIds[msg.sender][i]) {
                userTokenIds[msg.sender][i] = userTokenIds[msg.sender][len - 1];
                userTokenIds[msg.sender].pop();
                return true;
            }
        }
        return false;
    }


     function setRewardRate(uint256 _rewardRate) public onlyOwner {
         rewardRate = _rewardRate;
     }

    function emergencyToken(address token_, address to_, uint256 amount_) onlyOwner external {
        IERC20(token_).transfer(to_, amount_);
        emit EmergencyToken(token_, to_, amount_);
    }

    function emergencyTokenIds(address to_, uint256[] memory tokenIds) onlyOwner external {
        for (uint i = 0; i < tokenIds.length; i++) {
            nft.safeTransferFrom(address(this), to_, tokenIds[i]);
        }
        emit EmergencyTokenIds(to_, tokenIds);
    }
}
